{
  "name": "redcap-consort-report",
  "version": "0.1.2",
  "description": "Plugin for generating consort diagrams in REDCap",
  "main": "./dist/bundle.js",
  "scripts": {
    "clean": "rm -rf dist junit",
    "prebuild": "npm install",
    "build": "webpack --optimize-minimize",
    "test": "karma start karma.conf.js",
    "test-jenkins": "karma start karma.conf.js --single-run --reporters junit --browsers Chrome_Headless",
    "test-single-run": "karma start karma.conf.js --single-run",
    "start": "webpack --progress --colors --watch"
  },
  "author": "BMIP - OCTRI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://octriinternal.ohsu.edu/stash/scm/reddev/consort-report.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "~1.7.0",
    "eslint": "~4.10.0",
    "file-loader": "~1.1.5",
    "git-repo-info": "^1.4.1",
    "jasmine-ajax": "^3.4.0",
    "jasmine-core": "^2.8.0",
    "jasmine-jquery": "^2.1.1",
    "jquery": "~3.3.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.5",
    "moment": "^2.22.2",
    "mustache": "^2.3.2",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "uuid": "^3.3.2"
  }
