{
  "name": "report-counts",
  "version": "0.2.3",
  "description": "Plugin for generating report counts.",
  "main": "./dist/bundle.js",
  "scripts": {
    "analyze": "webpack --optimize-minimize; webpack --profile --json > bundle-stats.json; webpack-bundle-analyzer bundle-stats.json dist",
    "clean": "rm -rf dist junit bundle-stats.json",
    "lint:js": "eslint --ext .js --ext .vue ./*.js js js-tests",
    "prebuild": "npm install",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "test": "karma start karma.conf.js --single-run",
    "test-ci": "karma start karma.conf.js --single-run --browsers Chrome_Headless",
    "test-jenkins": "karma start karma.conf.js --single-run --reporters junit --browsers Chrome_Headless",
    "test-single-run": "karma start karma.conf.js --single-run",
    "start": "WATCHING=1 webpack --mode=development --progress --colors --watch"
  },
  "author": "BMIP - OCTRI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://octriinternal.ohsu.edu/stash/scm/reddev/report-counts.git"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.1.0",
    "git-repo-info": "^2.1.1",
    "html-webpack-plugin": "^4.4.1",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.6.0",
    "jquery": "^3.5.1",
    "karma": "^5.2.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.11.2",
    "moment": "^2.28.0",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "uuid": "^8.3.0",
    "vue": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
